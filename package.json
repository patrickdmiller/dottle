  "bugs": {
    "url": "https://github.com/patrickdmiller/dottle/issues"
  },
  "homepage": "https://github.com/patrickdmiller/dottle#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@protobuf-ts/grpc-backend": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "redis": "^4.6.15"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/redis": "^4.0.10",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
